# Telegram Userbot Configuration
# Получите эти данные на https://my.telegram.org/apps
TELEGRAM_API_ID=your_api_id_here
TELEGRAM_API_HASH=your_api_hash_here

# Telegram Phone Number (опционально)
# Ваш номер телефона в формате +79991234567
# Если не указан, будет запрошен при запуске
PHONE_NUMBER=+79991234567

# Google Gemini API Key
# Получите на https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here
